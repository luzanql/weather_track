{
    "name": "luzanql/weather_track",
    "description": "Api consum and exposure project",
    "type": "project",
    "require": {
        "php": "^7.3 || ^8.0",
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",
        "swiftmailer/swiftmailer": "^6.0",
        "php-di/php-di": "^6.3",
        "symfony/dotenv": "^5.2",
        "tuupola/slim-basic-auth": "^3.3",
        "php-amqplib/php-amqplib": "^3.0",
        "illuminate/database": "~5.1",
        "tuupola/slim-jwt-auth": "^3.6",
        "vlucas/valitron": "^1.4",
        "symfony/yaml": "^5.4",
        "slim/twig-view": "^3.3",
        "zircote/swagger-php": "^4.3",
        "ext-json": "*"
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
